{
  "name": "a_test",
  "version": "1.0.0",
  "main": "index.js",
  "title": "a_test",
  "description": "Sub package of a. Test framework in compact when-style. With recursive test runner",
  "keywords": [
    "tdd",
    "bdd",
    "test runner"
  ],
  "devDependencies": {
    "a_mock": "0.1.1",
    "babel-cli": "^6.3.17",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "glob": "^7.0.3",
    "globby": "^4.0.0",
    "install": "^0.5.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "babel-register": "^6.6.0",
    "styled-console": "^1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:alfateam/a_test.git"
  },
  "bin": {
    "a": "./dist/bin/a.js"
  },
  "scripts": {
    "build": "rm -rf ./dist ./test-compiled && babel ./src --out-dir ./dist -sq && babel ./test --out-dir ./test-compiled -q",
    "pretest": "npm run build",
    "test": "nodejs ./test-compiled/testAll.js",
    "prepublish": "npm run build"
  }
}
